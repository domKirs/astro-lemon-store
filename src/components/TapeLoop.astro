---
import Tape from "./Tape.astro";

const { class: classList, ...attr } = Astro.props;
---

<tape-loop class:list={[classList]} {...attr}>
  <Tape />
  <Tape />
</tape-loop>

<script>
  class TapeLoop extends HTMLElement {
    constructor() {
      super();
    }
  }

  customElements.define("tape-loop", TapeLoop);
</script>
